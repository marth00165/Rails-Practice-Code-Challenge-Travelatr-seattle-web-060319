<%=flash[:name_message]%>
<%=flash[:age_message]%>
<%=flash[:bio_message]%>
<h1>Create Blogger</h1>


<%= form_for @blogger do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.label :age %>
<%= f.number_field :age %><br>
<%= f.label :bio%>
<%= f.text_field :bio%><br>
<%= f.submit %>
<% end %>

<h3><%= link_to  "Back to Blogger List", bloggers_path%></h3>
